{
  "schemaVersion": 1,
  "id": "passableleaves",
  "version": "${version}",
  "name": "PassableLeaves",
  "description": "You want to run through leaves? With this mod its possible. It even let you run on them when having the right enchantment!",
  "authors": [
    "Kurisu"
  ],
  "contact": {
    "repo": "https://github.com/Kurisu-Null/PassableLeaves.git"
  },
  "license": "MIT",
  "icon": "assets/passableleaves/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.kurisu.passableleaves.client.PassableleavesClient"
    ],
    "main": [
      "me.kurisu.passableleaves.PassableLeaves"
    ]
  },
  "mixins": [
    "passableleaves.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric": "*",
    "minecraft": "1.20.0",
    "owo-lib": "^0.11.1"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/entity/player/PlayerEntity": ["me/kurisu/passableleaves/access/PlayerEntityAccess"],
      "net/minecraft/entity/Entity": ["me/kurisu/passableleaves/access/EntityAccess"]

    }
  }
}
